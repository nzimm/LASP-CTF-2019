#!/bin/sh
#
# custom wrapper for `ls` and `cat` system programs
#

# Helpful usage statement
read -r -d '' USAGE <<'EOF'
[ ls | cat ] <file path> ...
    ls
            list directory contents
    cat
            concatenate files and print on the standard output
EOF

# Parse commandline inputs
if [[ $# < 2 ]]; then
    echo -e "$0: error: command line argument required\n\n$USAGE"
# case statement on "ls" or "cat"
else
    echo "$0: error: not implemented"
fi
